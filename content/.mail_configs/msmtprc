# Set default values for all following accounts.
defaults
tls on
tls_starttls on
tls_certcheck off
logfile ~/.msmtp.log
auto_from on
maildomain threebean.org

# Gmail
account gmail
host smtp.gmail.com
port 587
auth on
user ralph.bean@gmail.com
password OMGSECRETZ

# red hat
account redhat
host smtp.corp.redhat.com
port 25
#auth on
user rbean@redhat.com
password OMGSECRETZ

# RIT mail
account rit
host smtp-server.rit.edu
port 25
auth on
user rjbpop
password OMGSECRETZ

# Set a default account
account default : gmail
